  "private": true,
  "homepage": "https://innovap3.github.io/js-exam/",
  "repository": {
    "type": "git",
    "url": "git@github.com:innovap3/js-exam.git"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/plugin-transform-react-inline-elements": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "3.3.4",
    "cross-env": "^5.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-redux-saga": "0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^4.13.1",
    "prettier": "1.19.1",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4"
  },
  "dependencies": {
    "@babel/standalone": "^7.1.0",
    "@sentry/browser": "^4.3.0",
    "antd": "^3.12.4",
    "aws-amplify": "^1.1.9",
    "aws-amplify-react": "^2.1.3",
    "backend-aws-exports": "^1.0.0",
    "blueimp-md5": "^2.10.0",
    "brace": "^0.11.1",
    "buble": "^0.19.6",
    "create-react-app": "2.1.2",
    "fsevents": "^2.1.2",
    "gh-pages": "^2.0.0",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^3.2.1",
    "husky": "^3.1.0",
    "invariant": "^2.2.4",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.0",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-ace": "^6.2.0",
    "react-dom": "16.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "^3.3.1",
    "react-stonecutter": "^0.3.9",
    "redux": "4.0.1",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.3.0",
    "sinon": "^2.3.5",
    "source-map-explorer": "^2.1.2",
    "tape": "^4.9.1",
    "tape-dom": "0.0.12"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,html,scss}": [
      "npm run format",
      "git add"
    ]
  },
  "scripts": {
    "format": "prettier \"src/**/*.{js,jsx,html,scss}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "start": "cross-env NODE_PATH=src/ react-scripts --max_old_space_size=4096 start",
    "build": "cross-env NODE_PATH=src/ react-scripts --max_old_space_size=4096 build",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build -r git@github.com:innovap3/js-exam.git",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
